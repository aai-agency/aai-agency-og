{
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "[python]": {
      "editor.formatOnSave": true
    },
    "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#84d2fb",
      "activityBar.background": "#84d2fb",
      "activityBar.foreground": "#15202b",
      "activityBar.inactiveForeground": "#15202b99",
      "activityBarBadge.background": "#f827b0",
      "activityBarBadge.foreground": "#e7e7e7",
      "commandCenter.border": "#15202b99",
      "sash.hoverBorder": "#84d2fb",
      "statusBar.background": "#52c0fa",
      "statusBar.foreground": "#15202b",
      "statusBarItem.hoverBackground": "#20aef9",
      "statusBarItem.remoteBackground": "#52c0fa",
      "statusBarItem.remoteForeground": "#15202b",
      "titleBar.activeBackground": "#52c0fa",
      "titleBar.activeForeground": "#15202b",
      "titleBar.inactiveBackground": "#52c0fa99",
      "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#52c0fa",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.terminal.activateEnvironment": true,
    "python.analysis.extraPaths": ["${workspaceFolder}"],
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "jupyter.interactiveWindow.creationMode": "perFile"
}